{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "API",
      "request": "launch",
      "runtimeArgs": ["artisan", "serve"],
      "runtimeExecutable": "php",
      "skipFiles": ["<node_internals>/**", "vendor/"],
      "type": "pwa-node",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Frontend",
      "request": "launch",
      "runtimeArgs": ["serve"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/vue_app"
    }
  ]
}
